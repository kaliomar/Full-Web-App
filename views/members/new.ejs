<% include ../partials/header%>
<div class="container">
  <div class="row">
    <div style="width: 30%; margin: 50px auto">
      <form action="/members" method="POST">
        <div class="form-group">
          <input class="form-control" type="text" placeholder="name" name="name">
        </div>
        <div class="form-group">
          <input class="form-control" type="number" placeholder="price" name="price" min="0.01" step="0.01">
        </div>
        <div class="form-group">
          <input class="form-control" type="text" placeholder="age" name="age">
        </div>
        <div class="form-group">
          <input class="form-control" type="text" placeholder="gender" name="gender">
        </div>
        <div class="form-group">
          <input class="form-control" type="text" placeholder="image url" name="image">
        </div>
        <div class="form-group">
          <button class="btn btn-primary btn-block form-control">Submit</button>
        </div>
      </form>
    </div>
  </div>
</div>

<% include ../partials/footer%>
